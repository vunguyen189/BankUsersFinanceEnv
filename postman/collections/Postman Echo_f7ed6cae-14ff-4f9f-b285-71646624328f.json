                                    "});",
                                    "",
                                    "pm.test(\"response body has json with request queries\", function () {",
                                    "    pm.response.to.have.jsonBody('args.foo1', 'bar1')",
                                    "        .and.have.jsonBody('args.foo2', 'bar2');",
                                    "});"
